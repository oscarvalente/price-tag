{
    "name": "price-tag",
    "version": "0.18.13",
    "description": "Records and tracks online shopping item prices",
    "main": "index.js",
    "repository": "git@bitbucket.org:oscarv_08/price-tag.git",
    "author": "Oscar Valente <oscarv_08@hotmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "7.1.2",
        "@babel/plugin-proposal-object-rest-spread": "7.0.0",
        "@babel/plugin-transform-async-to-generator": "7.1.0",
        "@babel/preset-react": "7.0.0",
        "node-sass": "4.9.3",
        "postcss-inline-svg": "3.1.1",
        "postcss-scss": "2.0.0",
        "rollup": "0.66.6",
        "rollup-plugin-babel": "4.0.3",
        "rollup-plugin-commonjs": "9.2.0",
        "rollup-plugin-node-resolve": "3.4.0",
        "rollup-plugin-postcss": "1.6.2",
        "rollup-plugin-replace": "2.1.0",
        "rollup-plugin-terser": "3.0.0"
    },
    "scripts": {
        "build": "yarn assemble && rollup -c --environment BUILD:production",
        "build:watch": "yarn assemble && rollup -c -w --environment BUILD:development",
        "assemble": "yarn clean && yarn copy:assets",
        "copy:assets": "cp -R manifest.json assets vendor views *.html *.css dist",
        "clean": "rm -rf dist && mkdir dist"
    },
    "engines": {
        "node": "= 8.11.1"
    },
    "dependencies": {
        "react": "16.5.2",
        "react-dom": "16.5.2"
    },
    "peerDependencies": {
        "react": "16.5.2",
        "react-dom": "16.5.2"
    }
}
