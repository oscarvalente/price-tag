  {
    "name": "Price Tag recorder",
    "version": "0.0.1",
    "description": "Records and tracks online shopping item prices",
    "manifest_version": 2,
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
